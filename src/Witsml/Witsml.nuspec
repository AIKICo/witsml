<?xml version="1.0" encoding="utf-8" ?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>PDS.Witsml</id>
    <version>$version$</version>
    <authors>Petrotechnical Data Systems</authors>
    <description>PDS WITSML Core Library</description>
    <projectUrl>https://witsml.pds.nl/</projectUrl>
    <licenseUrl>https://witsml.pds.nl/license</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <dependencies>
      <group targetFramework="net40">
        <dependency id="Energistics.DataAccess" version="2.0.20160722.1" />
      </group>
      <group targetFramework="net46">
        <dependency id="ETP.DevKit" version="1.1.20160801.1" />
        <dependency id="FluentValidationNA" version="1.2.16" />
        <dependency id="LinqExtender" version="3.0.1" />
        <dependency id="PDS.Framework" version="$BuildNumber$" />
        <dependency id="System.Linq.Dynamic.Library" version="1.1.14" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="bin/$configuration$/PDS.Witsml.dll" target="lib/net46" />
    <file src="bin/$configuration$/PDS.Witsml.pdb" target="lib/net46" />
    <file src="bin/$configuration$/PDS.Witsml.xml" target="lib/net46" />
  </files>
</package>
